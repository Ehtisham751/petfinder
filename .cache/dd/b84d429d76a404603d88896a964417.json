olumn":12}},{"generated":{"line":47,"column":23},"source":"js/main.js","original":{"line":65,"column":4}},{"name":"div","generated":{"line":47,"column":24},"source":"js/main.js","original":{"line":65,"column":24}},{"generated":{"line":47,"column":27},"source":"js/main.js","original":{"line":65,"column":4}},{"generated":{"line":48,"column":0},"source":"js/main.js","original":{"line":66,"column":3}},{"generated":{"line":48,"column":3},"source":"js/main.js","original":{"line":42,"column":2}},{"generated":{"line":49,"column":0},"source":"js/main.js","original":{"line":67,"column":1}}],"sources":{"js/main.js":"import fetchJsonp from \"fetch-jsonp\";\r\nimport { isValidZip, showAlert } from \"../validate\";\r\n\r\nconst petForm = document.getElementById(\"pet-form\");\r\n\r\npetForm.addEventListener(\"submit\", fetchAnimals);\r\n\r\n// Fetch Animals from api\r\nfunction fetchAnimals(e) {\r\n  // Get User Input\r\n  const animal = document.getElementById(\"animal\").value;\r\n  const zip = document.getElementById(\"zip\").value;\r\n\r\n  // Validate Zip\r\n  if (!isValidZip(zip)) {\r\n    showAlert(\"Please Enter A Valid Zipcode\", \"danger\");\r\n  } else {\r\n    const results = document.getElementById(\"results\");\r\n    results.innerHTML = \"<h1>Loading Please Wait...</h1>\";\r\n  }\r\n\r\n  fetchJsonp(\r\n    `http://api.petfinder.com/pet.find?format=json&key=7bec9c7514d4e1627003a8d320ba4286&animal=${animal}&location=${zip}&callback=pol`,\r\n    {\r\n      jsonpCallbackFunction: \"pol\",\r\n    }\r\n  )\r\n    .then((res) => res.json())\r\n    .then((data) => showAnimals(data.petfinder.pets.pet))\r\n    .catch((err) => console.log(err));\r\n\r\n  e.preventDefault();\r\n}\r\n\r\n// show listing of Pets\r\nfunction showAnimals(pets) {\r\n  console.log(pets);\r\n  const results = document.getElementById(\"results\");\r\n  results.innerHTML = \"\";\r\n\r\n  // LoopsThrough Pets\r\n  pets.forEach((pet) => {\r\n    const email = pet.contact.email.$t\r\n      ? pet.contact.email.$t\r\n      : `<span class=\"text-danger\">No available</span>`;\r\n    const div = document.createElement(\"div\");\r\n    div.classList.add(\"card\", \"card-body\", \"mb-3\");\r\n    div.innerHTML = `\r\n    <div class=\"row\">\r\n    <div class=\"col-sm-6\">\r\n      <h4>${pet.name.$t} (${pet.age.$t})</h4>\r\n      <p class=\"text-secondary\">${pet.breeds.breed.$t}</p>\r\n      <p>${pet.contact.address1.$t} ${pet.contact.city.$t} ${pet.contact.state.$t} ${pet.contact.zip.$t}</p>\r\n      <ul class=\"list-group\">\r\n        <li class=\"list-group-item\">Phone : ${pet.contact.phone.$t}</li>\r\n        <li class=\"list-group-item\">Email : ${email}</li>\r\n        <li class=\"list-group-item\">ID : ${pet.id.$t}</li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"col-sm-6 text-center\">\r\n    <img class=\"img-fluid rounded-circle mt-2\" src=\"${pet.media.photos.photo[3].$t}\"></img>\r\n    </div>\r\n  </div>\r\n    `;\r\n    results.appendChild(div);\r\n  });\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d81dd5d31ae7026f332d000f311b63df","cacheData":{"env":{}}}